<tal:capsalera define="utils context/@@upc.genweb.utils;
                         portal_url context/@@plone_portal_state/portal_url;">


                        
    <div class="logo">

        <img usemap="#logoUPC" title="Universitat Politècnica de Catalunya · BarcelonaTech" alt="Universitat Politècnica de Catalunya · BarcelonaTech"       
        tal:attributes="src string:${portal_url}/LogoUPC.png"/>
    
        <map name="logoUPC" id="logoUPC"><area shape="circle" coords="23,23,20" href="http://www.upc.edu" target="_blank" alt="Universitat Politècnica de Catalunya · BarcelonaTech" title="Universitat Politècnica de Catalunya · BarcelonaTech" /></map>

    </div>





    <div class="titolWeb titolWeb_n2">
			<h1>
        <a accesskey="1" href="" tal:attributes="href string:${view/portal_url}/">
            <!-- titol en catala -->
            <tal:titol_ca tal:content="python: utils.getGWConfig().titolespai_ca"  
								               tal:condition="python:utils.pref_lang()=='ca'">titolCAT</tal:titol_ca>
            <!-- titol en castella -->
            <tal:titol_es tal:content="python: utils.getGWConfig().titolespai_es" 
                               tal:condition="python:utils.pref_lang()=='es'">titolES</tal:titol_es>
            <!-- titol en angles -->
            <tal:titol_en tal:content="python: utils.getGWConfig().titolespai_en"
                               tal:condition="python:utils.pref_lang()=='en'">titolEN</tal:titol_en>
        </a>
			</h1>
    </div>
    
    



</tal:capsalera>


