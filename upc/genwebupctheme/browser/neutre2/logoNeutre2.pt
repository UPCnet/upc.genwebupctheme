<tal:capsaleraNeutre2 define="utils context/@@upc.genweb.utils;
                              portal_url context/@@plone_portal_state/portal_url;">

<div tal:define="portal_url context/@@plone_portal_state/portal_url;">
    <div class="logoNeutre2">
        <img  alt="logo UPC" longdesc="marca de la Universitat Politècnica de Catalunya" src="recerca_ca.jpg"               
               tal:condition="python:utils.pref_lang()=='ca'"
               tal:attributes="title python: utils.getGWConfig().titolespai_ca" />
        <img  alt="logo UPC" longdesc="marca de la Universitat Politècnica de Catalunya" src="recerca_es.jpg"               
               tal:condition="python:utils.pref_lang()=='es'"
               tal:attributes="title python: utils.getGWConfig().titolespai_es" />
        <img  alt="logo UPC" longdesc="marca de la Universitat Politècnica de Catalunya" src="recerca_en.jpg"               
               tal:condition="python:utils.pref_lang()=='en'"
               tal:attributes="title python: utils.getGWConfig().titolespai_en" />
    </div>
    <div class="titolWebNeutre2">
        <img src="imatgeNeutre2.jpg" longdesc="Títol de la Web" alt="Títol"/>
    </div>
    <div class="liniaPrincipalNeutre2"></div>
</div>
  
</tal:capsaleraNeutre2>