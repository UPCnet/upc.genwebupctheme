<!-- PORTLET AGENDA UPC -->

<tal:block tal:define="entries view/getRSS"
           tal:condition="entries">

<!-- CALCULAR FECHA -->
<div class="agendaDalt">Agenda </div>
<div class="agendaData">     
    <div class="agendaNumero" 
         tal:content="python:view.now[2]">
         number_day
    </div>
    <div class="agendaMes">
        <div class="agendaDia"
            tal:content="python:view.dia_semana(view.now[6])">
            text_day
        </div>
        <span tal:content="python:view.mes(view.now[1])">
        text_week_day </span>
    </div>
</div>
<!-- FIN CALCULAR FECHA -->
    
    <div class="agendaContingut agendaAjustament">
        <ul>
        <li tal:repeat="entry entries">
            <a href=""
                tal:attributes="title entry/summary | nothing;
                           href entry/url | nothing" target=newwindow>
                <span tal:replace="entry/summary | nothing">
                Notícies RSS
                </span>
            </a>
        </li>
        </ul>
        <div id="portletPlus">  
            <a href="#"> <img src="++resource++upc.genwebupctheme.images/custom/plus.gif" alt="més actualitat"> </a> 
            <a href="#"> més notícies </a> 
        </div>
    </div>

</tal:block>
<!-- FI PORTLET AGENDA UPC -->