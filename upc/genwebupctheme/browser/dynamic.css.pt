/*\*/
html*.actiuMenu{border-bottom:2px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}
.seleccionatMenu
	{border-right:2px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}
.liniaActivaEsq,
.liniaActivaDreta
	{border-top:2px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}



.baixMenu
	{color:<tal:propietat replace="e2">Especific2</tal:propietat>;}



.cantoES,
.cantoDR,
.cantoEC,
.cantoDC,
.portletHeader
	{background-header:<tal:propietat replace="e6">Especific6</tal:propietat>;}



.alphaCustom {border-right:2px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}

#portal-column-content fieldset > * input:focus,
#portal-column-content fieldset > * textarea:focus {
    border-color: <tal:propietat replace="e1">Especific1</tal:propietat>;}
</tal:dynamic>

