.template-event_view .vevent h1.summary 
	{color: <tal:propietat replace="e3">Especific3</tal:propietat> !important;}
input[id="title"]
	{color: <tal:propietat replace="e3">Especific3</tal:propietat>;}

tr.dragging td
	{background: <tal:propietat replace="e4">Especific4</tal:propietat>;}


blockquote
	{border-left:15px solid <tal:propietat replace="e5">Especific5</tal:propietat>;
	border-bottom: 1px solid <tal:propietat replace="e5">Especific5</tal:propietat>;}
td.draggable:hover
	{background:<tal:propietat replace="e5">Especific5</tal:propietat>;}


pre
	{background-color:<tal:propietat replace="e5">Especific5</tal:propietat>;}




#eRelacionats_border,
.notesCalendari
	{border-left:4px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}
.separadorV
	{border-left: 1px dashed <tal:propietat replace="e1">Especific1</tal:propietat>;}
.formulari input,
