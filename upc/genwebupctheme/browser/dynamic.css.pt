<tal:dynamic define="utils context/@@upc.genweb.utils;
                     e1 python:utils.getGWConfig().especific1;
                     e2 python:utils.getGWConfig().especific2;
                     e3 python:utils.getGWConfig().especific3;
                     e4 python:utils.getGWConfig().especific4;
                     e5 python:utils.getGWConfig().especific5;
                     e6 python:utils.getGWConfig().especific6;
                     portal_url view/portal_url;
                     language utils/pref_lang; 
                     es_anonim view/es_anonim;">

<tal:es_anonim condition="es_anonim">
    .navTreeItem a img {display:none}
</tal:es_anonim>

.submitGo {background: transparent url(cerca_<tal:propietat replace="language">language</tal:propietat>.gif) repeat scroll 0 0;}

#content a[href ^="<tal:propietat replace="portal_url">portal_url</tal:propietat>"], #content a.link-https { background:none; padding:0px; }

#content .columnaEsq a[href^="<tal:propietat replace="portal_url">portal_url</tal:propietat>"],
#content .columnaEsq a.link-https, #content .columnaDreta a[href^="<tal:propietat replace="portal_url">portal_url</tal:propietat>"],
#content .columnaDreta a.link-https {
    background:none; padding:0px;
}

input.password,
input.text,
input.searchField,
#searchGadget
	{border: 1px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}

#forgotten-password
	{border-left:3px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}

#signText
	{border-top: 3px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}
.bandaPeu
	{border-top: 18px solid <tal:propietat replace="e1">Especific1</tal:propietat>;}

.quadradet
	{background-color:<tal:propietat replace="e1">Especific1</tal:propietat>;}
.liniaPrincipal_m
	{border-color:<tal:propietat replace="e1">Especific1</tal:propietat>;}

blockquote,
#content .link-category
	{color: <tal:propietat replace="e2">Especific2</tal:propietat>;}
#bandaLogos
	{border-top:1px dashed <tal:propietat replace="e2">Especific2</tal:propietat>;}


label,
h1, h4, h5, h6,
h2.kssattr-atfieldname-title,
.divCarpetaTitol,
.modificacio,
.kssattr-atfieldname-title input,
