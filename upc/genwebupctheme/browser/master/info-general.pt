<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      xml:lang="en" lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="upc.genwebupc">
      
	<body>
	
		<metal:block metal:fill-slot="top_slot"
	    	         tal:define="dummy python:request.set('disable_border', 1)" />
	    	
		<metal:main fill-slot="main">
		    <tal:main-macro metal:define-macro="main"
       			tal:define="langtool nocall:context/portal_languages;
                  			lang_code langtool/getPreferredLanguage;
                  			global utils context/@@upc.genweb.utils;">
		
		
				<div tal:replace="structure provider:plone.abovecontenttitle" />
		
		
        		<h1 class="documentFirstHeading" tal:content="python: utils.recodifica(utils.getTextMaster('info_gral',lang_code))">Informació General</h1>
		
				<div class="plain" tal:define="data python: utils.getMasterInfoGeneral(utils.getGWConfig().idtitulacioMaster,utils.getGWConfig().idestudiMaster)">
											   
					<!-- OBJECTIUS DOCENT  -->
			        <div class="textDestacat"
			        	 tal:condition="python: data['objectius_docents_cat']"
			             tal:content="python: utils.recodifica(data['objectius_docents_'+utils.cambiaPrefijo(lang_code)])"> Servei de Comunicació i Promoció        
					</div>


					<div>&nbsp;</div>
					<table class="taulaDades">
					
					<!-- INTERUNIVERSITARI I ERASMUS MUNDUS -->
					
					        <tr tal:condition="python: (data['m_interuniversitari']==1) and (data['m_erasmus_mundus']==1)">
					            <td >&nbsp;</td>
            					<td tal:content="python: utils.recodifica(utils.getTextMaster('es_inter_erasmus',lang_code))">InterUniversitariErasmus</td>
					        </tr>
					
					<!-- INTERUNIVERSITARI -->
					
					        <tr tal:condition="python: (data['m_interuniversitari']==1)">
					            <td>&nbsp;</td>
            					<td tal:content="python: utils.recodifica(utils.getTextMaster('es_interuniversitari',lang_code))">InterUniversitariErasmus</td>
					        </tr>
					
					<!-- ERASMUS MUNDUS -->
					
					        <tr tal:condition="python: (data['m_erasmus_mundus']==1)">
					            <td>&nbsp;</td>
            					<td tal:content="python: utils.recodifica(utils.getTextMaster('es_erasmus',lang_code))">InterUniversitariErasmus</td>
					        </tr>

					<!-- ORIENTACIO -->
					
					        <tr tal:condition="python: utils.recodifica(data['orientacio_'+utils.cambiaPrefijo(lang_code)])">
					            <td tal:content="python: utils.recodifica(utils.getTextMaster('orientacio', lang_code))" class="titular2">Orientacio</td>
					            <td tal:content="python: utils.recodifica(data['orientacio_'+utils.cambiaPrefijo(lang_code)])">recerca</td>
					        </tr>					

					<!-- DATA INICI -->
					
					        <tr tal:condition="python: utils.recodifica(data['m_data_inici_'+utils.cambiaPrefijo(lang_code)])">
					            <td tal:content="python: utils.recodifica(utils.getTextMaster('inici_activitat', lang_code))" class="titular2">inici_activitat</td>
					            <td tal:content="python: utils.recodifica(data['m_data_inici_'+utils.cambiaPrefijo(lang_code)])">data_inici</td>
					        </tr>
					
					<!-- DURADA -->
					
					        <tr tal:condition="python: utils.recodifica(data['durada_' + utils.cambiaPrefijo(lang_code)])">
					            <td tal:content="python: utils.recodifica(utils.getTextMaster('durada', lang_code))" class="titular2">durada</td>
					            <td tal:content="python: utils.recodifica(data['durada_'+utils.cambiaPrefijo(lang_code)])">dosCursos</td>
					        </tr>
					
					<!-- CRÈDITS ECTS -->
					
					        <tr tal:condition="python: data['carrega_lectiva']">
					            <td tal:content="python: utils.recodifica(utils.getTextMaster('carrega_lectiva',lang_code))" class="titular2">durada</td>
					            <td tal:content="python: int(data['carrega_lectiva'])">120</td>
					        </tr>

					<!-- MODALITAT DOCENT -->
					
					        <tr tal:condition="python: utils.recodifica(data['up.nom_'+utils.cambiaPrefijo(lang_code)+' '+'pNom_'+utils.cambiaPrefijo(lang_code)])">
					            <td tal:content="python: utils.recodifica(utils.getTextMaster('modalitat_docent',lang_code))" class="titular2">modalitat</td>
					            <td tal:content="python: utils.recodifica(data['up.nom_'+utils.cambiaPrefijo(lang_code)+' '+'pNom_'+utils.cambiaPrefijo(lang_code)])">presencial</td>
					        </tr>

					<!-- IDIOMES -->
					
					        <tr tal:condition="python: utils.select_idiomes(utils.getGWConfig().idestudiMaster)">
					            <td tal:content="python: utils.recodifica(utils.getTextMaster('idiomes',lang_code))" class="titular2">idiomes</td>
					            <td>
					               <span tal:repeat="idiomes python: utils.select_idiomes(utils.getGWConfig().idestudiMaster)">
					                    <tal:last tal:define="is_last repeat/idiomes/end">
					                    <span tal:content="python: utils.recodifica(idiomes['ui.nom_'+utils.cambiaPrefijo(lang_code)])">catala</span><span tal:condition="python:not is_last">,</span>
					                    </tal:last>
					               </span>
					            </td>
					        </tr>

					<!-- UNITATS ORGANITZADORES -->
					
					        <tr tal:condition="python: utils.select_organitzadors(utils.getGWConfig().idestudiMaster)">
					            <td tal:content="python: utils.recodifica(utils.getTextMaster('unitats_organitzadores',lang_code))" class="titular2">unitats_organitzadores</td>
					            <td>
					               <span tal:repeat="organitzadors python:utils.select_organitzadors(utils.getGWConfig().idestudiMaster)" tal:omit-tag="">
					                     <div><a tal:attributes="href python: utils.recuperaLinkUnitats(organitzadors['uu.id_unitat'],lang_code)" target="_blank"
					                             tal:content="python: utils.recodifica(organitzadors['uu.nom_'+utils.cambiaPrefijo(lang_code)])">UPC</a>
					                     <span tal:condition="python:organitzadors['uu.sigles']"
					                           tal:content="python:'  ('+utils.recodifica(organitzadors['uu.sigles'])+')'"
					                           tal:omit-tag="">sigles</span></div>
					               </span>
					            </td>
					        </tr>

					<!-- SEPARACIO -->
					        <tr>
					            <td class="separadorH" colspan="2"></td>
					        </tr>
					        <tr>
					            <td colspan="2"></td>
					        </tr>
					        
					<!-- NOM TITULACIO -->
					
					        <tr tal:condition="python: utils.recodifica(data['m_nom_titulacio_'+utils.cambiaPrefijo(lang_code)])">
					            <td>&nbsp;</td>
					            <td tal:content="python: utils.recodifica(data['m_nom_titulacio_'+utils.cambiaPrefijo(lang_code)])" class="titular2">titulacio</td>
					        </tr>

					<!-- UNIVERSITATS PARTICIPANTS -->
					
					        <tr tal:condition="python: utils.select_participants(utils.getGWConfig().idestudiMaster)">
					            <td tal:content="python: utils.recodifica(utils.getTextMaster('universitats_participants',lang_code))" class="titular2">universitats_participants</td>
					            <td>
					               <span tal:repeat="participants python: utils.select_participants(utils.getGWConfig().idestudiMaster)" tal:omit-tag="">
					            <!-- Si no son universitats externes -->
					                             <div tal:repeat="externes python: utils.select_tipus(participants['uu.id_unitat'])">
					                                  <a tal:condition="python:externes['sut.id id']!=14"
					                                     tal:attributes="href python: utils.recuperaLinkUnitats(str(participants['uu.id_unitat']),lang_code)" target="_blank"
					                                     tal:content="python: utils.recodifica(participants['uu.nom_'+utils.cambiaPrefijo(lang_code)])">universitat</a>
					
					            <!-- Si son universitats externes -->
					  
					                                  <a tal:condition="python:(externes['sut.id id']==14) and (externes['web']!='http://')"
					                                     tal:attributes="href python:externes['web']" target="_blank"
					                                     tal:content="python: utils.recodifica(participants['uu.nom_'+utils.cambiaPrefijo(lang_code)])">universitat</a>
					
					                                  <span tal:condition="python:(externes['sut.id id']==14) and (externes['web']=='http://')"
					                                        tal:omit-tag=""
					                                        tal:content="python: utils.recodifica(participants['uu.nom_'+utils.cambiaPrefijo(lang_code)])">universitat</span>
					
					                             <span tal:condition="python: participants['uu.sigles']"
					                                   tal:content="python: '  (' + utils.recodifica(participants['uu.sigles']) + ')'"
					                                   tal:omit-tag="">sigles</span>
					                             </div>
					               </span>
					            </td>
					        </tr>

					<!-- UNIVERSITAT COORDINADORA -->
					
					        <tr tal:condition="python: data['unitat.nom_'+utils.cambiaPrefijo(lang_code)+' '+'uNom_'+utils.cambiaPrefijo(lang_code)]">
					            <td class="titular2" tal:content="python: utils.recodifica(utils.getTextMaster('universitat_coordinadora',lang_code))">universitat_coordinadora</td>
					            <td tal:content="python: utils.recodifica(data['unitat.nom_'+utils.cambiaPrefijo(lang_code)+' '+'uNom_'+utils.cambiaPrefijo(lang_code)])  + ' ('+utils.recodifica(data['unitat.sigles'])+')'">UPC</td>
					        </tr>
	
					<!-- MOBILITAT -->
					
					        <tr tal:condition="python: data['m_nom_titulacio_'+utils.cambiaPrefijo(lang_code)]">
					            <td class="titular2" tal:content="python: utils.recodifica(utils.getTextMaster('mobilitat',lang_code))">mobilitat</td>
					            <td tal:content="python: utils.recodifica(data['m_nom_titulacio_'+utils.cambiaPrefijo(lang_code)]) ">mobilitat</td>
					        </tr>

					<!-- DESTINATARIS -->
					
					        <tr tal:condition="python: data['m_destinataris_'+utils.cambiaPrefijo(lang_code)]">
					            <td class="titular2" tal:content="python: utils.recodifica(utils.getTextMaster('destinataris',lang_code))">destinataris</td>
					            <td tal:content="structure python: utils.recodifica(data['m_destinataris_'+utils.cambiaPrefijo(lang_code)])">destinataris</td>
					        </tr>

					<!-- LLOC IMPARTICIO -->
					
					        <tr tal:condition="python: data['m_lloc_imp_'+utils.cambiaPrefijo(lang_code)]">
					            <td class="titular2" tal:content="python: utils.recodifica(utils.getTextMaster('lloc_imparticio',lang_code))">lloc_imparticio</td>
					            <td tal:content="structure python: utils.recodifica(data['m_lloc_imp_'+utils.cambiaPrefijo(lang_code)])">lloc</td>
					        </tr>

					<!-- PROFESSORAT -->
					
					        <tr tal:condition="python: data['m_professorat_'+utils.cambiaPrefijo(lang_code)]">
					            <td class="titular2" tal:content="python: utils.recodifica(utils.getTextMaster('professorat',lang_code))">professorat</td>
					            <td tal:content="python: utils.recodifica(data['m_professorat_'+utils.cambiaPrefijo(lang_code)])">profes</td>
					        </tr>

					<!-- PREU -->
					
					        <tr>
					            <td class="titular2" tal:content="python: utils.recodifica(utils.getTextMaster('preu',lang_code))">preu</td>
					            <td>
					               <span tal:condition="python: data['m_erasmus_mundus']==1"
					                     tal:content="python: utils.recodifica(utils.getTextMaster('preu_erasmus',lang_code))">preu_erasmus</span>
					
					               <span tal:condition="python: data['m_erasmus_mundus']!=1"
					                     tal:content="python: utils.recodifica(utils.getTextMaster('preu_no_erasmus',lang_code))">preu_no_erasmus</span>
					            </td>
					        </tr>

					<!-- PLACES -->
					
					        <tr tal:condition="python: data['places']">
					            <td class="titular2" tal:content="python: utils.recodifica(utils.getTextMaster('places',lang_code))">places</td>
					            <td tal:content="python: data['places']">20</td>
					        </tr>

					<!-- CONTACTE -->
					
					        <tr tal:condition="python: data['m_web']">
					            <td class="titular2">Web</td>
					            <td><a tal:attributes="href python: data['m_web']" target="_blank" tal:content="python: data['m_web']">www.upc.edu</a></td>
					        </tr>
					
					        <tr tal:condition="python: data['m_email']">
					            <td class="titular2" tal:content="python: utils.recodifica(utils.getTextMaster('a_e',lang_code))">a_e</td>
					            <td>
					               <tal:repe tal:repeat="mail python: data['m_email'].split(';')">
					                                 <div><a tal:attributes="href python:'mailto:'+ mail" target="_blank"
					                                     tal:content="python:mail">www@edu</a>
					                                 </div>
					                                 </tal:repe>
					            </td>
					        </tr>
        					
					</table>

				
				</div>
			</tal:main-macro>
		</metal:main>
	
	</body>
</html>