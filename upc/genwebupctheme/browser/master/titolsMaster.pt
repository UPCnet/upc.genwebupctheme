<tal:capsalera define="utils context/@@upc.genweb.utils;
                         portal_url context/@@plone_portal_state/portal_url;">

<div class="clearAll_m">                         
    <div class="banerMasters">
    <span tal:condition="python: utils.getGWConfig().masterdoctorado==('Master')">
        <a href="" tal:attributes="href string:${view/portal_url}/">
        <!-- titol en catala -->
            <img border="0" usemap="#MapaMasters"
               tal:condition="python:utils.pref_lang()=='ca'"
               tal:attributes="title python: utils.getGWConfig().titolespai_ca;
                                src string:${portal_url}/logo_masters_ca.jpg" />
 
        <!-- titol en castella -->
            <img border="0" usemap="#MapaMasters"
               tal:condition="python:utils.pref_lang()=='es'"
               tal:attributes="title python: utils.getGWConfig().titolespai_es;
                                src string:${portal_url}/logo_masters_es.jpg" />
        <!-- titol en angles -->
            <img border="0" usemap="#MapaMasters"             
               tal:condition="python:utils.pref_lang()=='en'"
               tal:attributes="title python: utils.getGWConfig().titolespai_en;
                                src string:${portal_url}/logo_masters_en.jpg" />
                                
			<map name="MapaMD" id="MapaMD">
            	<area shape="rect" coords="48,19,170,46" href="#masters" target="_blank" alt="Màsters Universitaris" />
    		</map>              
		</a>
	</span>    		
    		
    <span tal:condition="python: utils.getGWConfig().masterdoctorado==('Master i Doctorat')">
        <a href="" tal:attributes="href string:${view/portal_url}/">
        <!-- titol en catala -->
            <img width="172" height="65" border="0" usemap="#MapaMD"
               tal:condition="python:utils.pref_lang()=='ca'"
               tal:attributes="title python: utils.getGWConfig().titolespai_ca;
                                src string:${portal_url}/logo_masters_doctorat_ca.jpg" />
        <!-- titol en castella -->
            <img width="172" height="65" border="0" usemap="#MapaMD"
               tal:condition="python:utils.pref_lang()=='es'"
               tal:attributes="title python: utils.getGWConfig().titolespai_es;
                                src string:${portal_url}/logo_masters_doctorat_es.jpg" />
        <!-- titol en angles -->
            <img width="172" height="65" border="0" usemap="#MapaMD"             
               tal:condition="python:utils.pref_lang()=='en'"
               tal:attributes="title python: utils.getGWConfig().titolespai_en;
                                src string:${portal_url}/logo_masters_doctorat_en.jpg" />
                                
			<map name="MapaMD" id="MapaMD">
				<area shape="rect" coords="48,2,171,29" href="#masters" target="_blank" alt="màsters universitaris" /><area shape="rect" coords="59,32,169,63" href="#doctorat" target="_blank" alt="doctorat" />
			</map>                            
			
	</a>
	</span>     		
    		
    </div>                         
    <div class="logo_m">
        <img usemap="#logoUPC" title="Universitat Politècnica de Catalunya"  
        tal:attributes="src string:${portal_url}/LogoUPC.gif"/>
        <map name="logoUPC" id="logoUPCMaster"><area shape="circle" coords="23,23,20" href="http://www.upc.edu" target="_blank" alt="Universitat Politècnica de Catalunya" /></map>
        <a tal:attributes="href python: utils.getGWConfig().enllaslogotip" target="_blank">
            <!-- titol en catala -->
            <img 
                tal:condition="python:utils.pref_lang()=='ca'" 
                tal:attributes="title python: utils.getGWConfig().firmaunitat_ca;
                src string:${portal_url}/logoPropi.gif"/>
            <!-- titol en castella -->
            <img 
                tal:condition="python:utils.pref_lang()=='es'"
                tal:attributes="title python: utils.getGWConfig().firmaunitat_es;
                src string:${portal_url}/logoPropi.gif"/>
            <!-- titol en angles -->
            <img 
                tal:condition="python:utils.pref_lang()=='en'"
                tal:attributes="title python: utils.getGWConfig().firmaunitat_en;
                src string:${portal_url}/logoPropi.gif"/>
        </a>
    </div>
    <div class="titolWeb_m">
        <a href="" tal:attributes="href string:${view/portal_url}/">
            <!-- titol en catala -->
            <img longdesc="Títol de la Web" src="titolWebMaster_ca.gif"               
               tal:condition="python:utils.pref_lang()=='ca'"
               tal:attributes="title python: utils.getGWConfig().titolespai_ca" />
            <!-- titol en castella -->
            <img longdesc="Títol de la Web" src="titolWebMaster_es.gif"               
               tal:condition="python:utils.pref_lang()=='es'"
               tal:attributes="title python: utils.getGWConfig().titolespai_es" />
            <!-- titol en angles -->
            <img longdesc="Títol de la Web" src="titolWebMaster_en.gif"               
               tal:condition="python:utils.pref_lang()=='en'"
               tal:attributes="title python: utils.getGWConfig().titolespai_en" />
        </a>
        <br/>

        <span class="cursMaster" tal:condition="python:utils.pref_lang()=='ca'" tal:content="python: utils.getGWConfig().edicio_ca"> 2008/2009 </span>
        <span class="cursMaster" tal:condition="python:utils.pref_lang()=='es'" tal:content="python: utils.getGWConfig().edicio_es"> 2008/2009 </span>
        <span class="cursMaster" tal:condition="python:utils.pref_lang()=='en'" tal:content="python: utils.getGWConfig().edicio_en"> 2008/2009  </span>

    </div>

</div>

</tal:capsalera>
