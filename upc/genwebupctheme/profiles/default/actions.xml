<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_workflow"/>
 <action-provider name="portal_types">
  <action title="Log" action_id="log" category="object"
     condition_expr="nocall: object/@@log|nothing"
     url_expr="string:$object_url/log" visible="True">
   <permission>View</permission>
  </action>
 </action-provider>
 <action-provider name="portal_actions"/>
  
 <object name="portal_tabs" meta_type="CMF Action Category">
   <property name="visible">False</property>
 </object>
 
 <object name="accessibility" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
 </object>

 <object name="plone_setup" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
 </object>
 
</object>